{
    "name": "webignition/tcp-cli-proxy-server",
    "description": "Proxy server (for running inside docker container) to allow proxying of CLI commands over TCP",
    "keywords": [],
    "homepage": "https://github.com/webignition/tcp-cli-proxy-server",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jon Cram",
            "email": "webignition@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "webignition\\TcpCliProxyServer\\": "src",
            "webignition\\TcpCliProxyServer\\Bin\\": "bin"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "webignition\\TcpCliProxyServer\\Tests\\": "tests"
        }
    },
    "scripts": {
        "cs": [
            "./vendor/bin/ecs check src tests --ansi --config ecs.yaml",
            "./vendor/bin/phpcs"
        ],
        "cs-fix": [
            "./vendor/bin/ecs check src tests --ansi --config ecs.yaml --fix"
        ],
        "static-analysis": "./vendor/bin/phpstan analyse --ansi",
        "test": "./vendor/bin/phpunit --colors=always",
        "ci": [
            "@cs",
            "@static-analysis",
            "@test"
        ],
        "phar-dump-autoload": "@composer dump-autoload --classmap-authoritative --no-dev --no-scripts --optimize",
        "phar-compile": "@php -dphar.readonly=0 bin/compile.php",
        "phar-build": [
            "@composer install --no-dev --prefer-dist",
            "rm -f build/server.phar",
            "@phar-dump-autoload",
            "@phar-compile",
            "rm -f server",
            "cp build/server.phar server",
            "chmod +x ./server",
            "@composer install"
        ],
        "server-test": [
            "@composer dump-autoload",
            "./vendor/bin/phpunit --colors=always tests/Integration/ServerTest.php"
        ]
    },
    "require": {
        "php": ">=7.4",
        "ext-pcntl": "*",
        "ext-sockets": "*",
        "clue/socket-raw": "^1.4",
        "monolog/monolog": "^2.1",
        "webignition/single-command-application-phar-builder": "^0.2.0",
        "webignition/tcp-cli-proxy-models": "^0.2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "php-mock/php-mock": "^2.1",
        "php-mock/php-mock-mockery": "^1.3",
        "phpstan/extension-installer": "^1.0",
        "phpstan/phpstan": "^0.12.0",
        "phpstan/phpstan-mockery": "^0.12.0",
        "phpunit/phpunit": "^9.2",
        "squizlabs/php_codesniffer": "3.*",
        "symplify/easy-coding-standard": "^8.0",
        "webignition/tcp-cli-proxy-client": "^0.1.0"
    },
    "bin": [
        "bin/compiler"
    ],
    "config": {
        "sort-packages": true
    }
}
